{
  "name": "sales-intelligence-ai",
  "version": "1.0.0",
  "description": "AI-powered sales intelligence platform with real-time search and analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "clean": "rimraf dist",
    "cdk": "cdk",
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "deploy": "npm run build && npm run cdk:deploy",
    "cache:migrate": "node scripts/migrate-cache-types.js",
    "cache:migrate:dry-run": "node scripts/migrate-cache-types.js --dry-run"
  },
  "keywords": [
    "sales",
    "intelligence",
    "ai",
    "typescript",
    "perplexity",
    "search",
    "aws",
    "cdk",
    "lambda"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-lambda": "^3.840.0",
    "@aws-sdk/client-secrets-manager": "^3.848.0",
    "@aws-sdk/client-sfn": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0",
    "aws-cdk-lib": "^2.124.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "constructs": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "robots-parser": "^3.0.1",
    "source-map-support": "^0.5.21",
    "url-parse": "^1.5.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/url-parse": "^1.4.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-cdk": "^2.124.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
